    </style>
     <style name="pw_anim_style">
        <!-- 设置pw显示时的动画 -->
        <item name="android:windowEnterAnimation">@anim/pw_show</item>
      	<!-- 设置pw隐藏时的动画 -->
        <item name="android:windowExitAnimation">@anim/pw_hide</item>
    </style>
      <style name="pw_anim_style2">
        <!-- 设置pw显示时的动画 -->
        <item name="android:windowEnterAnimation">@anim/pw_show2</item>
      	<!-- 设置pw隐藏时的动画 -->
        <item name="android:windowExitAnimation">@anim/pw_hide2</item>
    </style>
